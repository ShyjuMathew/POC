{
    "rules": {
      "strict": "error",
      "no-debugger": "error",
      "no-empty": "error",
      "block-scoped-var": "error",
      "complexity": ["error", 15],
      "default-case": "error",
      "eqeqeq": "error",
      "no-empty-function": "error",
      "no-eval": "error",
      "no-redeclare": "error",
      "no-unused-vars": "error",
      "no-const-assign": "error",
      "no-duplicate-imports": "error",
      "no-var": "error",
      "prefer-const": "error"
    }
  }
  